<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.human.boot.dao.ChunjaDAO">
	<insert id="insert" parameterType="ChunjaVO">
		insert into chunja values(#{idx},#{A},#{B},#{C})
	</insert>
	<select id="selectCount" resultType="int">
		select count(*) from chunja
	</select>
	<select id="selectByIdx" parameterType="int" resultType="ChunjaVO">
		select * from chunja where idx=#{idx}
	</select>
</mapper>  